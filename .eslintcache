lastName,\n      email,\n      password,\n      confirmPassword,\n    } = this.state;\n\n    if (password !== confirmPassword) {\n      alert(\"password does not match\");\n      return;\n    }\n    try {\n      await axios.post(\"http://localhost:5000/users\", this.state);\n      this.setState({\n        firstName: \"\",\n        lastName: \"\",\n        email: \"\",\n        password: \"\",\n        confirmPassword: \"\",\n      });\n      alert(\"successfully created user. Please login.\");\n    } catch (e) {\n      console.log(e);\n    }\n  };\n  handleChange = (e) => {\n    const { name, value } = e.target;\n    this.setState({ [name]: value });\n  };\n  render() {\n    return (\n      <div className=\"register\">\n        <h2>CREATE ACCOUNT</h2>\n        <form onSubmit={this.handleSubmit}>\n          <FormInput\n            name=\"firstName\"\n            type=\"text\"\n            value={this.state.firstName}\n            require={true}\n            label=\"first name\"\n            handleChange={this.handleChange}\n          />\n          <FormInput\n            name=\"lastName\"\n            type=\"text\"\n            value={this.state.lastName}\n            require={true}\n            label=\"last name\"\n            handleChange={this.handleChange}\n          />\n          <FormInput\n            name=\"email\"\n            type=\"email\"\n            value={this.state.email}\n            require={true}\n            label=\"email\"\n            handleChange={this.handleChange}\n          />\n          <FormInput\n            name=\"password\"\n            type=\"password\"\n            value={this.state.password}\n            require={true}\n            label=\"password\"\n            handleChange={this.handleChange}\n          />\n          <FormInput\n            name=\"confirmPassword\"\n            type=\"password\"\n            value={this.state.confirmPassword}\n            require={true}\n            label=\"confirm password\"\n            handleChange={this.handleChange}\n          />\n          <CustomButton type=\"submit\">Create Account</CustomButton>\n        </form>\n      </div>\n    );\n  }\n}\n\nexport default Register;\n",["255","256"],"/Users/seandan/ecommerce-app/src/components/form-input/form-input.component.js",[],"/Users/seandan/ecommerce-app/src/components/login/login.component.js",[],"/Users/seandan/ecommerce-app/src/redux/user/user.reducer.js",[],"/Users/seandan/ecommerce-app/src/redux/user/user.actions.js",[],["257","258"],"/Users/seandan/ecommerce-app/src/redux/root-reducer.js",[],"/Users/seandan/ecommerce-app/src/redux/user/user.types.js",[],"/Users/seandan/ecommerce-app/src/components/cart-icon/cart-icon.component.js",[],["259","260"],"/Users/seandan/ecommerce-app/src/redux/cart/cart.actions.js",[],"/Users/seandan/ecommerce-app/src/redux/cart/cart.types.js",[],["261","262"],"/Users/seandan/ecommerce-app/src/redux/cart/cart.reducer.js",[],"/Users/seandan/ecommerce-app/src/components/cart-dropdown/cart-dropdown.component.js",[],"/Users/seandan/ecommerce-app/src/components/add-bag-popover/add-bag-popover.component.js",[],"/Users/seandan/ecommerce-app/src/utils/clickOutside.js",[],["263","264"],"/Users/seandan/ecommerce-app/src/utils/cart.js",[],"/Users/seandan/ecommerce-app/src/components/cart-item/cart-item.component.js",[],["265","266"],"/Users/seandan/ecommerce-app/src/redux/user/user.selectors.js",[],"/Users/seandan/ecommerce-app/src/components/custom-button/custom-button.component.js",[],"/Users/seandan/ecommerce-app/src/redux/store.js",[],"/Users/seandan/ecommerce-app/src/pages/checkout/checkout.component.js",[],"/Users/seandan/ecommerce-app/src/components/checkout-items/checkout-items.component.js",[],"/Users/seandan/ecommerce-app/src/components/checkout-header/checkout-header.component.js",[],"/Users/seandan/ecommerce-app/src/redux/cart/cart.selectors.js",[],"/Users/seandan/ecommerce-app/src/components/checkout-item/checkout-item.component.js",[],"/Users/seandan/ecommerce-app/src/components/checkout-payment/checkout-payment.component.js",[],"/Users/seandan/ecommerce-app/src/components/checkout-form/checkout-form.component.js",["267","268"],"import React, { useState } from \"react\";\nimport { CardElement, useStripe, useElements } from \"@stripe/react-stripe-js\";\nimport axios from \"axios\";\nimport { useHistory } from \"react-router-dom\";\nimport { connect } from \"react-redux\";\n\nimport \"./checkout-form.styles.scss\";\nimport { Button } from \"@chakra-ui/react\";\nimport { clearCart } from \"../../redux/cart/cart.actions\";\nimport FormInput from \"../form-input/form-input.component\";\n\nconst CARD_OPTIONS = {\n  iconStyle: \"solid\",\n  style: {\n    base: {\n      iconColor: \"#000\",\n      color: \"#000\",\n      fontWeight: 500,\n      fontFamily: \"Roboto, Open Sans, Segoe UI, sans-serif\",\n      fontSize: \"16px\",\n      fontSmoothing: \"antialiased\",\n      \":-webkit-autofill\": { color: \"#fce883\" },\n      \"::placeholder\": { color: \"#808080\" },\n    },\n    invalid: {\n      iconColor: \"#ffc7ee\",\n      color: \"#ffc7ee\",\n    },\n  },\n};\n\nconst INIT_STATE = {\n  firstName: \"\",\n  lastName: \"\",\n  email: \"\",\n  address1: \"\",\n  address2: \"\",\n  city: \"\",\n  state: \"\",\n  zipcode: \"\",\n};\n\nconst CheckoutForm = ({ status, setStatus, total, clearCart }) => {\n  const stripe = useStripe();\n  const elements = useElements();\n  const [customerInfo, setCustomerInfo] = useState(INIT_STATE);\n  const history = useHistory();\n\n  const handleSubmit = async (event) => {\n    event.preventDefault();\n    setStatus(\"loading\");\n    const { error, paymentMethod } = await stripe.createPaymentMethod({\n      type: \"card\",\n      card: elements.getElement(CardElement),\n    });\n\n    if (!error) {\n      const { id } = paymentMethod;\n      try {\n        const { data } = await axios.post(\"http://localhost:5000/payments\", {\n          id,\n          amount: total,\n          customer: customerInfo,\n        });\n        setStatus(\"success\");\n        clearCart();\n        //clear cart and redirect to home page\n        // setTimeout(() => {\n        //   history.push(\"/\");\n        // }, 2000);\n      } catch (error) {\n        console.log(error);\n        setStatus(\"fail\");\n      }\n    } else {\n      console.log(error);\n      setStatus(\"fail\");\n    }\n  };\n\n  return (\n    <form onSubmit={handleSubmit} className=\"checkout-form\">\n      <CardElement options={CARD_OPTIONS} />\n      <div className=\"form-rows\">\n        <FormInput\n          name=\"email\"\n          type=\"email\"\n          value={customerInfo.email}\n          require={true}\n          label=\"email\"\n          handleChange={(e) =>\n            setCustomerInfo({ ...customerInfo, email: e.target.value })\n          }\n        />\n        <div className=\"name-row\">\n          <FormInput\n            name=\"firstName\"\n            type=\"text\"\n            value={customerInfo.firstName}\n            require={true}\n            label=\"John\"\n            handleChange={(e) =>\n              setCustomerInfo({ ...customerInfo, firstName: e.target.value })\n            }\n          />\n          <FormInput\n            name=\"lastName\"\n            type=\"text\"\n            value={customerInfo.lastName}\n            require={true}\n            label=\"Doe\"\n            handleChange={(e) =>\n              setCustomerInfo({ ...customerInfo, lastName: e.target.value })\n            }\n          />\n        </div>\n\n        <FormInput\n          name=\"address1\"\n          type=\"text\"\n          value={customerInfo.address1}\n          require={true}\n          label=\"Address 1\"\n          handleChange={(e) =>\n            setCustomerInfo({ ...customerInfo, address1: e.target.value })\n          }\n        />\n        <FormInput\n          name=\"address2\"\n          type=\"text\"\n          value={customerInfo.address2}\n          require={true}\n          label=\"Address 2 (optional)\"\n          handleChange={(e) =>\n            setCustomerInfo({ ...customerInfo, address2: e.target.value })\n          }\n        />\n        <div className=\"city-state-zip-row\">\n          <FormInput\n            name=\"city\"\n            type=\"text\"\n            value={customerInfo.city}\n            require={true}\n            label=\"city\"\n            handleChange={(e) =>\n              setCustomerInfo({ ...customerInfo, city: e.target.value })\n            }\n          />\n          <FormInput\n            name=\"state\"\n            type=\"text\"\n            value={customerInfo.state}\n            require={true}\n            label=\"state\"\n            handleChange={(e) =>\n              setCustomerInfo({ ...customerInfo, state: e.target.value })\n            }\n          />\n          <FormInput\n            name=\"zipcode\"\n            type=\"text\"\n            value={customerInfo.zipcode}\n            require={true}\n            label=\"Zipcode\"\n            handleChange={(e) =>\n              setCustomerInfo({ ...customerInfo, zipcode: e.target.value })\n            }\n          />\n        </div>\n      </div>\n      <div className=\"checkout-button\">\n        <Button\n          isLoading={status === \"loading\"}\n          className=\"submit\"\n          type=\"submit\"\n          disabled={!stripe}\n        >\n          Pay\n        </Button>\n      </div>\n    </form>\n  );\n};\n\nconst mapDispatchToProps = (dispatch) => ({\n  clearCart: () => dispatch(clearCart()),\n});\n\nexport default connect(null, mapDispatchToProps)(CheckoutForm);\n","/Users/seandan/ecommerce-app/src/components/loading-spinner/loading-spinner.styles.js",[],"/Users/seandan/ecommerce-app/src/components/loading-spinner/loading-spinner.component.js",[],"/Users/seandan/ecommerce-app/src/effects/use-fetch.effect.js",[],"/Users/seandan/ecommerce-app/src/utils/auth.js",[],["269","270"],"/Users/seandan/ecommerce-app/src/pages/profile/profile.component.js",[],"