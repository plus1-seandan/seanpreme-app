);\n    setPage(0);\n    setHasMore(true);\n  }, [collectionId]);\n\n  useEffect(() => {\n    if (collection.items.length === 0) {\n      loadMoreItems();\n    }\n  }, [collection]);\n\n  async function loadMoreItems() {\n    setLoading(true);\n    setTimeout(async () => {\n      const { data } = await axios({\n        method: \"GET\",\n        url: `http://localhost:5000/collections?id=${collectionId}`,\n        params: { _page: page, _limit: 12, _collectionId: collectionId },\n      });\n      setCollection({\n        collection: data.collection,\n        items: [...collection.items, ...data.items.items],\n      });\n      setPage((prevPageNumber) => prevPageNumber + 1);\n      setHasMore(data.items.items.length > 0);\n      setLoading(false);\n    }, 1000);\n  }\n\n  return (\n    <div className=\"shop-page\">\n      <div className=\"shop-page-body\">\n        <div className=\"collection-filter\">COLLECTION FILTER</div>\n        <div className=\"collection-main\">\n          <Collection isLoading={isLoading} collection={collection} />\n          {!isLoading && HasMore && (\n            <div className=\"load-more-button\">\n              <CustomButton onClick={loadMoreItems}>Load more</CustomButton>\n            </div>\n          )}\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default ShopPage;\n",["207","208"],"/Users/seandan/ecommerce-app/src/components/collection/collection.component.js",[],"/Users/seandan/ecommerce-app/src/routes.js",[],"/Users/seandan/ecommerce-app/src/components/collection-item/collection-item.component.js",["209","210","211","212"],"import React, { useEffect, useRef, useState } from \"react\";\nimport { connect } from \"react-redux\";\nimport { addItem } from \"../../redux/cart/cart.actions\";\nimport { useClickOutside } from \"../../utils/clickOutside\";\nimport AddBagPopover from \"../add-bag-popover/add-bag-popover.component\";\n\nimport CustomButton from \"../custom-button/custom-button.component\";\nimport \"./collection-item.styles.scss\";\n\nconst CollectionItem = ({ item, addItem }) => {\n  const { imageUrl, itemName, price } = item;\n  const [showPopover, setShowPopover] = useState(false);\n\n  let domNode = useClickOutside(() => {\n    setShowPopover(false);\n  });\n\n  return (\n    <div className=\"collection__item\" ref={domNode}>\n      <div className=\"collection__item__image\">\n        <img src={imageUrl} alt={item.itemName} />\n      </div>\n      <div className=\"collection__item__footer\">\n        <span className=\"name\">{itemName}</span>\n        <span className=\"price\">${price} USD</span>\n        <CustomButton onClick={() => setShowPopover(!showPopover)}>\n          ADD TO BAG\n        </CustomButton>\n        <AddBagPopover\n          show={showPopover}\n          setShowPopover={setShowPopover}\n          item={item}\n        />\n      </div>\n    </div>\n  );\n};\n\nexport default CollectionItem;\n",["213","214"],"/Users/seandan/ecommerce-app/src/pages/login-register/login-register.components.js",[],["215","216"],"/Users/seandan/ecommerce-app/src/components/register/register.component.js",["217","218","219"],"/Users/seandan/ecommerce-app/src/components/form-input/form-input.component.js",[],["220","221"],"/Users/seandan/ecommerce-app/src/components/login/login.component.js",[],"/Users/seandan/ecommerce-app/src/redux/user/user.reducer.js",[],"/Users/seandan/ecommerce-app/src/redux/user/user.actions.js",[],["222","223"],"/Users/seandan/ecommerce-app/src/redux/root-reducer.js",[],"/Users/seandan/ecommerce-app/src/redux/user/user.types.js",[],"/Users/seandan/ecommerce-app/src/components/cart-icon/cart-icon.component.js",[],"/Users/seandan/ecommerce-app/src/redux/cart/cart.actions.js",[],"/Users/seandan/ecommerce-app/src/redux/cart/cart.types.js",[],"/Users/seandan/ecommerce-app/src/redux/cart/cart.reducer.js",[],"/Users/seandan/ecommerce-app/src/components/cart-dropdown/cart-dropdown.component.js",[],["224","225"],"/Users/seandan/ecommerce-app/src/components/