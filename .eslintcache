    setCustomerInfo({ ...customerInfo, firstName: e.target.value })\n            }\n          />\n          <FormInput\n            name=\"lastName\"\n            type=\"text\"\n            value={customerInfo.lastName}\n            require={true}\n            label=\"Doe\"\n            handleChange={(e) =>\n              setCustomerInfo({ ...customerInfo, lastName: e.target.value })\n            }\n          />\n        </div>\n\n        <FormInput\n          name=\"address1\"\n          type=\"text\"\n          value={customerInfo.address1}\n          require={true}\n          label=\"Address 1\"\n          handleChange={(e) =>\n            setCustomerInfo({ ...customerInfo, address1: e.target.value })\n          }\n        />\n        <FormInput\n          name=\"address2\"\n          type=\"text\"\n          value={customerInfo.address2}\n          require={true}\n          label=\"Address 2 (optional)\"\n          handleChange={(e) =>\n            setCustomerInfo({ ...customerInfo, address2: e.target.value })\n          }\n        />\n        <div className=\"city-state-zip-row\">\n          <FormInput\n            name=\"city\"\n            type=\"text\"\n            value={customerInfo.city}\n            require={true}\n            label=\"city\"\n            handleChange={(e) =>\n              setCustomerInfo({ ...customerInfo, city: e.target.value })\n            }\n          />\n          <FormInput\n            name=\"state\"\n            type=\"text\"\n            value={customerInfo.state}\n            require={true}\n            label=\"state\"\n            handleChange={(e) =>\n              setCustomerInfo({ ...customerInfo, state: e.target.value })\n            }\n          />\n          <FormInput\n            name=\"zipcode\"\n            type=\"text\"\n            value={customerInfo.zipcode}\n            require={true}\n            label=\"Zipcode\"\n            handleChange={(e) =>\n              setCustomerInfo({ ...customerInfo, zipcode: e.target.value })\n            }\n          />\n        </div>\n      </div>\n      <div className=\"checkout-button\">\n        <Button\n          isLoading={status === \"loading\"}\n          className=\"submit\"\n          type=\"submit\"\n          disabled={!stripe}\n        >\n          Pay\n        </Button>\n      </div>\n    </form>\n  );\n};\n\nconst mapDispatchToProps = (dispatch) => ({\n  clearCart: () => dispatch(clearCart()),\n});\n\nexport default connect(null, mapDispatchToProps)(CheckoutForm);\n","/Users/seandan/ecommerce-app/src/components/loading-spinner/loading-spinner.styles.js",[],"/Users/seandan/ecommerce-app/src/components/loading-spinner/loading-spinner.component.js",[],"/Users/seandan/ecommerce-app/src/effects/use-fetch.effect.js",[],"/Users/seandan/ecommerce-app/src/utils/auth.js",[],"/Users/seandan/ecommerce-app/src/pages/profile/profile.component.js",[],"/Users/seandan/ecommerce-app/src/components/profile-main/profile-main.component.js",["229"],{"ruleId":"230","severity":1,"message":"231","line":3,"column":8,"nodeType":"232","messageId":"233","endLine":3,"endColumn":14},{"ruleId":"234","replacedBy":"235"},{"ruleId":"236","replacedBy":"237"},{"ruleId":"234","replacedBy":"238"},{"ruleId":"236","replacedBy":"239"},{"ruleId":"234","replacedBy":"240"},{"ruleId":"236","replacedBy":"241"},{"ruleId":"234","replacedBy":"242"},{"ruleId":"236","replacedBy":"243"},{"ruleId":"234","replacedBy":"244"},{"ruleId":"236","replacedBy":"245"},{"ruleId":"230","severity":1,"message":"246","line":4,"column":10,"nodeType":"232","messageId":"233","endLine":4,"endColumn":17},{"ruleId":"247","severity":1,"message":"248","line":34,"column":6,"nodeType":"249","endLine":34,"endColumn":18,"suggestions":"250"},{"ruleId":"234","replacedBy":"251"},{"ruleId":"236","replacedBy":"252"},{"ruleId":"230","severity":1,"message":"253","line":1,"column":17,"nodeType":"232","messageId":"233","endLine":1,"endColumn":26},{"ruleId":"230","severity":1,"message":"254","line":1,"column":28,