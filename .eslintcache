 <span className=\"price\">${price * quantity}</span>\n      </div>\n    </div>\n  );\n};\n\nconst mapDispatchToProps = (dispatch) => ({\n  clearItem: (item) => dispatch(clearItem(item)),\n  addItem: (item) => dispatch(addItem(item)),\n  removeItem: (item) => dispatch(removeItem(item)),\n});\n\nexport default connect(null, mapDispatchToProps)(CheckoutItem);\n","/Users/seandan/ecommerce-app/src/components/checkout-payment/checkout-payment.component.js",[],"/Users/seandan/ecommerce-app/src/components/checkout-form/checkout-form.component.js",["317","318"],"import React, { useState } from \"react\";\nimport { CardElement, useStripe, useElements } from \"@stripe/react-stripe-js\";\nimport axios from \"axios\";\nimport { useHistory } from \"react-router-dom\";\nimport { connect } from \"react-redux\";\n\nimport \"./checkout-form.styles.scss\";\nimport { Button } from \"@chakra-ui/react\";\nimport { clearCart } from \"../../redux/cart/cart.actions\";\nimport FormInput from \"../form-input/form-input.component\";\n\nconst CARD_OPTIONS = {\n  iconStyle: \"solid\",\n  style: {\n    base: {\n      iconColor: \"#000\",\n      color: \"#000\",\n      fontWeight: 500,\n      fontFamily: \"Roboto, Open Sans, Segoe UI, sans-serif\",\n      fontSize: \"16px\",\n      fontSmoothing: \"antialiased\",\n      \":-webkit-autofill\": { color: \"#fce883\" },\n      \"::placeholder\": { color: \"#808080\" },\n    },\n    invalid: {\n      iconColor: \"#ffc7ee\",\n      color: \"#ffc7ee\",\n    },\n  },\n};\n\nconst INIT_STATE = {\n  firstName: \"\",\n  lastName: \"\",\n  email: \"\",\n  address1: \"\",\n  address2: \"\",\n  city: \"\",\n  state: \"\",\n  zipcode: \"\",\n};\n\nconst CheckoutForm = ({ status, setStatus, total, clearCart }) => {\n  const stripe = useStripe();\n  const elements = useElements();\n  const [customerInfo, setCustomerInfo] = useState(INIT_STATE);\n  const history = useHistory();\n\n  const handleSubmit = async (event) => {\n    event.preventDefault();\n    setStatus(\"loading\");\n    const { error, paymentMethod } = await stripe.createPaymentMethod({\n      type: \"card\",\n      card: elements.getElement(CardElement),\n    });\n\n    if (!error) {\n      const { id } = paymentMethod;\n      try {\n        const { data } = await axios.post(\n          `${process.env.REACT_APP_SERVER_URL}/payments`,\n          {\n            id,\n            amount: total,\n            customer: customerInfo,\n          }\n        );\n        setStatus(\"success\");\n        clearCart();\n      } catch (error) {\n        console.log(error);\n        setStatus(\"fail\");\n      }\n    } else {\n      console.log(error);\n      setStatus(\"fail\");\n    }\n  };\n\n  return (\n    <form onSubmit={handleSubmit} className=\"checkout-form\">\n      <CardElement options={CARD_OPTIONS} />\n      <div className=\"form-rows\">\n        <FormInput\n          name=\"email\"\n          type=\"email\"\n          value={customerInfo.email}\n          require={true}\n          label=\"email\"\n          handleChange={(e) =>\n            setCustomerInfo({ ...customerInfo, email: e.target.value })\n          }\n        />\n        <div className=\"name-row\">\n          <FormInput\n            name=\"firstName\"\n            type=\"text\"\n            value={customerInfo.firstName}\n            require={true}\n            label=\"John\"\n            handleChange={(e) =>\n              setCustomerInfo({ ...customerInfo, firstName: e.target.value })\n            }\n          />\n          <FormInput\n            name=\"lastName\"\n            type=\"text\"\n            value={customerInfo.lastName}\n            require={true}\n            label=\"Doe\"\n            handleChange={(e) =>\n              setCustomerInfo({ ...customerInfo, lastName: e.target.value })\n            }\n          />\n        </div>\n\n        <FormInput\n          name=\"address1\"\n          type=\"text\"\n          value={customerInfo.address1}\n          require={true}\n          labe