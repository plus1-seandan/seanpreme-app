llection-item.component.js",["215","216","217","218"],"import React, { useEffect, useRef, useState } from \"react\";\nimport { connect } from \"react-redux\";\nimport { addItem } from \"../../redux/cart/cart.actions\";\nimport { useClickOutside } from \"../../utils/clickOutside\";\nimport AddBagPopover from \"../add-bag-popover/add-bag-popover.component\";\n\nimport CustomButton from \"../custom-button/custom-button.component\";\nimport \"./collection-item.styles.scss\";\n\nconst CollectionItem = ({ item, addItem }) => {\n  const { imageUrl, itemName, price } = item;\n  const [showPopover, setShowPopover] = useState(false);\n\n  let domNode = useClickOutside(() => {\n    setShowPopover(false);\n  });\n\n  return (\n    <div className=\"collection__item\" ref={domNode}>\n      <div className=\"collection__item__image\">\n        <img src={imageUrl} alt={item.itemName} />\n      </div>\n      <div className=\"collection__item__footer\">\n        <span className=\"name\">{itemName}</span>\n        <span className=\"price\">${price} USD</span>\n        <CustomButton onClick={() => setShowPopover(!showPopover)}>\n          ADD TO BAG\n        </CustomButton>\n        <AddBagPopover\n          show={showPopover}\n          setShowPopover={setShowPopover}\n          item={item}\n        />\n      </div>\n    </div>\n  );\n};\n\nexport default CollectionItem;\n",["219","220"],"/Users/seandan/ecommerce-app/src/pages/login-register/login-register.components.js",[],["221","222"],"/Users/seandan/ecommerce-app/src/components/register/register.component.js",["223","224","225"],"import React from \"react\";\nimport CustomButton from \"../custom-button/custom-button.component\";\nimport FormInput from \"../form-input/form-input.component\";\nimport axios from \"axios\";\n\nimport \"./register.styles.scss\";\n\nclass Register extends React.Component {\n  constructor(props) {\n    super(props);\n\n    this.state = {\n      firstName: \"\",\n      lastName: \"\",\n      email: \"\",\n      password: \"\",\n      confirmPassword: \"\",\n    };\n  }\n  handleSubmit = async (e) => {\n    e.preventDefault();\n    const {\n      firstName,\n      lastName,\n      email,\n      password,\n      confirmPassword,\n    } = this.state;\n\n    if (password !== confirmPassword) {\n      alert(\"password does not match\");\n      return;\n    }\n    try {\n      await axios.post(\"http://localhost:5000/users\", this.state);\n      this.setState({\n        firstName: \"\",\n        lastName: \"\",\n        email: \"\",\n        password: \"\",\n        confirmPassword: \"\",\n      });\n      alert(\"successfully created user. Please login.\");\n    } catch (e) {\n      console.log(e);\n    }\n  };\n  handleChange = (e) => {\n    const { name, value } = e.target;\n    this.setState({ [name]: value });\n  };\n  render() {\n    return (\n      <div className=\"register\">\n        <h2>CREATE ACCOUNT</h2>\n        <form onSubmit={this.handleSubmit}>\n          <FormInput\n            name=\"firstName\"\n            type=\"text\"\n            value={this.state.firstName}\n            require={true}\n            label=\"first name\"\n            handleChange={this.handleChange}\n          />\n          <FormInput\n            name=\"lastName\"\n            type=\"text\"\n            value={this.state.lastName}\n            require={true}\n            label=\"last name\"\n            handleChange={this.handleChange}\n          />\n          <FormInput\n            name=\"email\"\n            type=\"email\"\n            value={this.state.email}\n            require={true}\n            label=\"email\"\n            handleChange={this.handleChange}\n          />\n          <FormInput\n            name=\"password\"\n            type=\"password\"\n            value={this.state.password}\n            require={true}\n            label=\"password\"\n            handleChange={this.handleChange}\n          />\n          <FormInput\n            name=\"confirmPassword\"\n            type=\"password\"\n            value={this.state.confirmPassword}\n            require={true}\n            label=\"confirm password\"\n            handleChange={this.handleChange}\n          />\n          <CustomButton type=\"submit\">Create Account</CustomButton>\n        </form>\n      </div>\n    );\n  }\n}\n\nexport default Register;\n",["226","227"],"/Users/seandan/ecommerce-app/src/components/form-input/form-input.component.js",[],["228","229"],"/Users/seandan/ecommerce-app/src/components/login/login.component.js",[],"/Users/seandan/ecommerce-app/src/redux/user/user.reducer.js",[],"/Users/seandan/ecommerce-app/src/redux/user/user.actions.js",[],["230","231"],"/Users/seandan/ecommerce-app/src/redux/root-reducer.js",[],["232","233"],"/Users/seandan/ecommerce-app/src/redux/user/user.types.js",[],"/Users/seandan/ecommerce-app/src/components/cart-icon/cart-icon.component.js",[],"/Users/seandan/ecommerce-app/src/redux/cart/cart.actions.js",[],"/Users/seandan/ecommerce-app/src/redux/cart/cart.types.js",[],"/Users/seandan/ecommerce-app/src/redux/cart/cart.reducer.js",[],"/Users/seandan/ecommerce-app/src/components/cart-dropdown/cart-dropdown.component.js",[],["234","235"],"/Users/seandan/ecommerce-app/src/components/add-bag-popover/add-bag-popover.component.js",[],"/Users/seandan/ecommerce-app/src/utils/clickOutside.js",[],"/Users/seandan/ecommerce-app/src/utils/cart.js",[],"/Users/seandan/ecommerce-app/src/components/cart-item/cart-item.component.js",[],"/Users/seandan/ecommerce-app/src/redux/user/user.selectors.js",[],"/Users/seandan/ecommerce-app/src/components/custom-button/custom-button.component.js",[],"/Users/seandan/ecommerce-app/src/redux/store.js",[],"/Users/seandan/ecommerce-app/src/pages/checkout/checkout.component.js",[],"/Users/seandan/ecommerce-app/src/components/checkout-items/checkout-items.component.js",[],"/Users/seandan/ecommerce-app/src/components/checkout-header/checkout-header.component.js",[],"/Users/seandan/ecommerce-app/src/redux/cart/cart.selectors.js",[],"/Users/seandan/ecommerce-app/src/components/checkout-item/checkout-item.component.js",[],"/Users/seandan/ecommerce-app/src/components/checkout-payment/checkout-payment.component.js",[],["236","237"],"/Users/seandan/ecommerce-app/src/components/checkout-form/checkout-form.component.js",["238"],"import React, { useState } from \"react\";\nimport { CardElement, useStripe, useElements } from \"@stripe/react-stripe-js\";\nimport axios from \"axios\";\nimport { useHistory } from \"react-router-dom\";\nimport { connect } from \"react-redux\";\n\nimport \"./checkout-form.styles.scss\";\nimport { Button } from \"@chakra-ui/react\";\nimport { clearCart } from \"../../redux/cart/cart.actions\";\nimport FormInput from \"../form-input/form-input.component\";\n\nconst CARD_OPTIONS = {\n  iconStyle: \"solid\",\n  style: {\n    base: {\n      iconColor: \"#000\",\n      color: \"#000\",\n      fontWeight: 500,\n      fontFamily: \"Roboto, Open Sans, Segoe UI, sans-serif\",\n      fontSize: \"16px\",\n      fontSmoothing: \"antialiased\",\n      \":-webkit-autofill\": { color: \"#fce883\" },\n      \"::placeholder\": { color: \"#808080\" },\n    },\n    invalid: {\n      iconColor: \"#ffc7ee\",\n      color: \"#ffc7ee\",\n    },\n  },\n};\n\nconst INIT_STATE = {\n  firstName: \"\",\n  lastName: \"\",\n  email: \"\",\n  address1: \"\",\n  address2: \"\",\n  city: \"\",\n  state: \"\",\n  zipcode: \"\",\n};\n\nconst CheckoutForm = ({ status, setStatus, total, clearCart }) => {\n  const stripe = useStripe();\n  const elements = useElements();\n  const [customerInfo, setCustomerInfo] = useState(INIT_STATE);\n  const history = useHistory();\n\n  const handleSubmit = async (event) => {\n    event.preventDefault();\n    setStatus(\"loading\");\n    const { error, paymentMethod } = await stripe.createPaymentMethod({\n      type: \"card\",\n      card: elements.getElement(CardElement),\n    });\n\n    if (!error) {\n      const { id } = paymentMethod;\n      try {\n        const { data } = await axios.post(\"http://localhost:5000/payments\", {\n          id,\n          amount: total,\n          customer: customerInfo,\n        });\n        setStatus(\"success\");\n        clearCart();\n        //clear cart and redirect to home page\n        setTimeout(() => {\n          history.push(\"/\");\n        }, 2000);\n      } catch (error) {\n        console.log(error);\n        setStatus(\"fail\");\n      }\n    } else {\n      console.log(error);\n      setStatus(\"fail\");\n    }\n  };\n\n  return (\n    <form onSubmit={handleSubmit} className=\"checkout-form\">\n      <CardElement options={CARD_OPTIONS} />\n      <div className=\"form-rows\">\n        <FormInput\n          name=\"email\"\n          type=\"email\"\n          value={customerInfo.email}\n          require={true}\n          label=\"email\"\n          handleChange={(e) =>\n            setCustomerInfo({ ...customerInfo, email: e.target.value })\n          }\n        />\n        <div className=\"name-row\">\n          <FormInput\n            name=\"firstName\"\n            type=\"text\"\n            value={customerInfo.firstName}\n            require={true}\n            label=\"John\"\n            handleChange={(e) =>\n              setCustomerInfo({ ...customerInfo, firstName: e.target.value })\n            }\n          />\n          <FormInput\n            name=\"lastName\"\n            type=\"text\"\n            value={customerInfo.lastName}\n            require={true}\n            label=\"Doe\"\n            handleChange={(e) =>\n              setCustomerInfo({ ...customerInfo, lastName: e.target.value })\n            }\n          />\n        </div>\n\n        <FormInput\n          name=\"address1\"\n          type=\"text\"\n          value={customerInfo.address1}\n          require={true}\n          label=\"Address 1\"\n          handleChange={(e) =>\n            setCustomerInfo({ ...customerInfo, address1: e.target.value })\n          }\n        />\n        <FormInput\n          name=\"address2\"\n          type=\"text\"\n          value={customerInfo.address2}\n          require={true}\n          label=\"Address 2 (optional)\"\n          handleChange={(e) =>\n            setCustomerInfo({ ...customerInfo, address2: e.target.value })\n          }\n        />\n        <div className=\"city-state-zip-row\">\n          <FormInput\n            name=\"city\"\n            type=\"text\"\n            value={customerInfo.city}\n            require={true}\n            label=\"city\"\n            handleChange={(e) =>\n              setCustomerInfo({ ...customerInfo, city: e.target.value })\n            }\n          />\n          <FormInput\n            name=\"state\"\n            type=\"text\"\n            value={customerInfo.state}\n            require={true}\n            label=\"state\"\n            handleChange={(e) =>\n              setCustomerInfo({ ...customerInfo, state: e.target.value })\n            }\n          />\n          <FormInput\n            name=\"zipcode\"\n            type=\"text\"\n            value={customerInfo.zipcode}\n            require={true}\n            label=\"Zipcode\"\n            handleChange={(e) =>\n              setCustomerInfo({ ...customerInfo, zipcode: e.target.value })\n            }\n          />\n        </div>\n      </div>\n      <div className=\"checkout-button\">\n        <Button\n          isLoading={status === \"loading\"}\n          className=\"submit\"\n          type=\"submit\"\n          disabled={!stripe}\n        >\n          Pay\n        </Button>\n      </div>\n    </form>\n  );\n};\n\nconst mapDispatchToProps = (dispatch) => ({\n  clearCart: () => dispatch(clearCart()),\n});\n\nexport default connect(null, mapDispatchToProps)(CheckoutForm);\n","/Users/seandan/ecommerce-app/src/components/loading-spinner/loading-spinner.styles.js",[],"/Users/seandan/ecommerce-app/src/components/loading-spinner/loading-spinner.component.js",[],"/Users/seandan/ecommerce-app/src/effects/use-fetch.effect.js",[],"/Users/seandan/ecommerce-app/src/utils/auth.js",[],"/Users/seandan/ecommerce-app/src/pages/profile/profile.component.js",[],"/Users/seandan/ecommerce-app/src/components/profile-main/profile-main.component.js",["239"],"import { connect } from \"react-redux\";\nimport axios from \"axios\";\nimport {\n  Table,\n  Thead,\n  Tbody,\n  Tfoot,\n  Tr,\n  Th,\n  Td,\n  TableCaption,\n} from \"@chakra-ui/react\";\nimport { withRouter } from \"react-router-dom\";\n\nimport CustomButton from \"../custom-button/custom-button.component\";\nimport \"./profile-main.styles.scss\";\nimport { logoutUser } from \"../../redux/user/user.actions\";\nimport { clearCart } from \"../../redux/cart/cart.actions\";\n\nconst ProfileMain = ({ user, logoutUser, history, clearCart }) => {\n  const handleLogout = async () => {\n    try {\n      //backend call to destroy session\n      await axios.post(\"http://localhost:5000/users/logout\", null, {\n        withCredentials: \"include\",\n      });\n      //action to remove user from redux store\n      // logoutUser();\n      //redirect to home\n      history.push(\"/\");\n      logoutUser();\n      clearCart();\n    } catch (e) {\n      console.log(e);\n    }\n  };\n  return (\n    <div className=\"profile-main\">\n      <h1 className=\"greeting\">Hello, {user.firstName}</h1>\n      <CustomButton onClick={handleLogout}>Logout</CustomButton>\n      <p className=\"description\">\n        From your dashboard you have the ability to view a snapshot of your\n        recent account activity and update your account information. Select a\n        link below to view or edit information.\n      </p>\n      <div className=\"account-address-info\">\n        <div className=\"account-information\">\n          <h2>ACCOUNT INFORMATION</h2>\n          <h3>Contact Information</h3>\n          <span>\n            Name: {user.firstName} {user.lastName}\n          </span>\n          <span>Email: {user.email}</span>\n        </div>\n        <div className=\"address-book\">\n          <h2>ADDRESS BOOK</h2>\n          <h3>Addresses</h3>\n        </div>\n      </div>\n      <div className=\"my-orders\">\n        <h2>My Orders</h2>\n        <Table variant=\"simple\">\n          <Thead>\n            <Tr>\n              <Th>Order</Th>\n              <Th>Date</Th>\n              <Th>Payment Status</Th>\n              <Th>Fullfillment Status</Th>\n              <Th>Total</Th>\n            </Tr>\n          </Thead>\n          <Tbody>\n            <Tr>\n              <Td>#61919214</Td>\n              <Td>November 3, 2020</Td>\n              <Td>Paid</Td>\n              <Td>Fullfilled</Td>\n              <Td>$85.46 USD</Td>\n            </Tr>\n          </Tbody>\n          <Tfoot></Tfoot>\n        </Table>\n      </div>\n    </div>\n  );\n};\n\nconst mapDispatchToProps = (dispatch) => ({\n  logoutUser: () => dispatch(logoutUser()),\n  clearCart: () => dispatch(clearCart()),\n});\n\nexport default withRouter(connect(null, mapDispatchToProps)(ProfileMain));\n","/Users/seandan/ecommerce-app/src/components/collection-filter/collection-filter.component.js",[],"/Users/seandan/ecommerce-app/src/components/footer/footer.component.js",[],{"ruleId":"240","severity":1,"message":"241","line":3,"column":8,"nodeType":"242","messageId":"243","endLine":3,"endColumn":14},{"ruleId":"244","replacedBy":"245"},{"ruleId":"246","replacedBy":"247"},{"ruleId":"244","replacedBy":"248"},{"ruleId":"246","replacedBy":"249"},{"ruleId":"244","replacedBy":"250"},{"ruleId":"246","replacedBy":"251"},{"ruleId":"240","severity":1,"message":"252","line":14,"column":8,"nodeType":"242","messageId":"243","endLine":14,"endColumn":12},{"ruleId":"244","replacedBy":"253"},{"ruleId":"246","replacedBy":"254"},{"ruleId":"240","severity":1,"message":"255","line":4,"column":10,"nodeType":"242","messageId":"243","endLine":4,"endColumn":17},{"ruleId":"256","severity":1,"message":"257","line":35,"column":6,"nodeType":"258","endLine":35,"endColumn":18,"suggestions":"259"},{"ruleId":"240",