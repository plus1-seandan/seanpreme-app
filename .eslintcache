styles.scss\";\n\nconst HomePage = () => (\n  <div className=\"homepage\">\n    <Directory />\n  </div>\n);\n\nexport default HomePage;\n",["216","217"],"/Users/seandan/ecommerce-app/src/components/directory/directory.component.js",[],"/Users/seandan/ecommerce-app/src/components/menu-item/menu-item.component.js",[],["218","219"],"/Users/seandan/ecommerce-app/src/index.js",[],["220","221"],"/Users/seandan/ecommerce-app/src/components/header/header.component.js",["222"],"/Users/seandan/ecommerce-app/src/pages/shop/shop.component.js",["223","224","225"],"/Users/seandan/ecommerce-app/src/components/collection/collection.component.js",["226"],"import React from \"react\";\nimport { Select, Spinner } from \"@chakra-ui/react\";\n\nimport CollectionItem from \"../collection-item/collection-item.component\";\nimport \"./collection.styles.scss\";\n\nconst Collection = ({ isLoading, collection }) => {\n  if (!collection || !collection.collection) {\n    return (\n      <div className=\"load-items-spinner\">\n        <span>loading...</span>\n        <Spinner size=\"xl\" />\n      </div>\n    );\n  }\n  return (\n    <div className=\"collection\">\n      <div className=\"collection__items\">\n        {collection.items.map((item) => (\n          <CollectionItem key={item.id} item={item} />\n        ))}\n      </div>\n      {isLoading && (\n        <div className=\"load-more-spinner\">\n          <Spinner size=\"xl\" />\n        </div>\n      )}\n    </div>\n  );\n};\n\nexport default Collection;\n","/Users/seandan/ecommerce-app/src/routes.js",[],"/Users/seandan/ecommerce-app/src/components/collection-item/collection-item.component.js",[],"/Users/seandan/ecommerce-app/src/pages/login-register/login-register.components.js",[],["227","228"],"/Users/seandan/ecommerce-app/src/components/register/register.component.js",["229","230","231"],"import React from \"react\";\nimport CustomButton from \"../custom-button/custom-button.component\";\nimport FormInput from \"../form-input/form-input.component\";\nimport axios from \"axios\";\n\nimport \"./register.styles.scss\";\n\nclass Register extends React.Component {\n  constructor(props) {\n    super(props);\n\n    this.state = {\n      firstName: \"\",\n      lastName: \"\",\n      email: \"\",\n      password: \"\",\n      confirmPassword: \"\",\n    };\n  }\n  handleSubmit = async (e) => {\n    e.preventDefault();\n    const {\n      firstName,\n      lastName,\n      email,\n      password,\n      confirmPassword,\n    } = this.state;\n\n    if (password !== confirmPassword) {\n      alert(\"password does not match\");\n      return;\n    }\n    try {\n      await axios.post(\"http://localhost:5000/users\", this.state);\n      this.setState({\n        firstName: \"\",\n        lastName: \"\",\n        email: \"\",\n        password: \"\",\n        confirmPassword: \"\",\n      });\n      alert(\"successfully created user. Please login.\");\n    } catch (e) {\n      console.log(e);\n    }\n  };\n  handleChange = (e) => {\n    const { name, value } = e.target;\n    this.setState({ [name]: value });\n  };\n  render() {\n    return (\n      <div className=\"register\">\n        <h2>CREATE ACCOUNT</h2>\n        <form onSubmit={this.handleSubmit}>\n          <FormInput\n            name=\"firstName\"\n            type=\"text\"\n            value={this.state.firstName}\n            require={true}\n            label=\"first name\"\n            handleChange={this.handleChange}\n          />\n          <FormInput\n            name=\"lastName\"\n            type=\"text\"\n            value={this.state.lastName}\n            require={true}\n            label=\"last name\"\n            handleChange={this.handleChange}\n          />\n          <FormInput\n            name=\"email\"\n            type=\"email\"\n            value={this.state.email}\n            require={true}\n            label=\"email\"\n            handleChange={this.handleChange}\n          />\n          <FormInput\n            name=\"password\"\n            type=\"password\"\n            value={this.state.password}\n            require={true}\n            label=\"password\"\n            handleChange={this.handleChange}\n          />\n          <FormInput\n            name=\"confirmPassword\"\n            type=\"password\"\n            value={this.state.confirmPassword}\n            require={true}\n            label=\"confirm password\"\n            handleChange={this.handleChange}\n          />\n          <CustomButton type=\"submit\">Create Account</CustomButton>\n        </form>\n      </div>\n    );\n  }\n}\n\nexport default Register;\n",["232","233"],"/Users/seandan/ecommerce-app/src/components/form-input/form-input.component.js",[],"/Users/seandan/ecommerce-app/src/components/login/login.component.js",[],"/Users/seandan/ecommerce-app/src/redux/user/user.reducer.js",[],"/Users/seandan/ecommerce-app/src/redux/user/user.actions.js",[],["234","235"],"/Users/seandan/ecommerce-app/src/redux/root-reducer.js",[],"/Users/seandan/ecommerce-app/src/redux/user/user.types.js",[],"/Users/seandan/ecommerce-app/src/components/cart-icon/cart-icon.component.js",[],["236","237"],"/Users/seandan/ecommerce-app/src/redux/cart/cart.actions.js",[],["238","239"],"/Users/seandan/ecommerce-app/src/redux/cart/cart.types.js",[],["240","241"],"/Users/seandan/ecommerce-app/src/redux/cart/cart.reducer.js",[],"/Users/seandan/ecommerce-app/src/components/cart-dropdown/cart-dropdown.component.js",[],"/Users/seandan/ecommerce-app/src/components/add-bag-popover/add-bag-popover.component.js",[],"/Users/seandan/ecommerce-app/src/utils/clickOutside.js",[],["242","243"],"/Users/seandan/ecommerce-app/src/utils/cart.js",[],"/Users/seandan/ecommerce-app/src/components/cart-item/cart-item.component.js",[],["244","245"],"/Users/seandan/ecommerce-app/src/redux/user/user.selectors.js",[],"/Users/seandan/ecommerce-app/src/components/custom-button/custom-button.component.js",[],"/Users/seandan/ecommerce-app/src/redux/store.js",["246"],"import { createStore, applyMiddleware } from \"redux\";\nimport logger from \"redux-logger\";\nimport { persistStore } from \"redux-persist\";\nimport rootReducer from \"./root-reducer\";\n\n//logger\nconst middlewares = [];\n\nexport const store = createStore(rootReducer, applyMiddleware(...middlewares));\n\nexport const persistor = persistStore(store);\n","/Users/seandan/ecommerce-app/src/pages/checkout/checkout.component.js",[],"/Users/seandan/ecommerce-app/src/components/checkout-items/checkout-items.component.js",[],"/Users/seandan/ecommerce-app/src/components/checkout-header/checkout-header.component.js",[],"/Users/seandan/ecommerce-app/src/redux/cart/cart.selectors.js",[],"/Users/seandan/ecommerce-app/src/components/checkout-item/checkout-item.component.js",[],"/Users/seandan/ecommerce-app/src/components/checkout-payment/checkout-payment.component.js",[],"/Users/seandan/ecommerce-app/src/components/checkout-form/checkout-form.component.js",["247","248"],"import React, { useState } from \"react\";\nimport { CardElement, useStripe, useElements } from \"@stripe/react-stripe-js\";\nimport axios from \"axios\";\nimport { useHistory } from \"react-router-dom\";\nimport { connect } from \"react-redux\";\n\nimport \"./checkout-form.styles.scss\";\nimport { Button } from \"@chakra-ui/react\";\nimport { clearCart } from \"../../redux/cart/cart.actions\";\nimport FormInput from \"../form-input/form-input.component\";\n\nconst CARD_OPTIONS = {\n  iconStyle: \"solid\",\n  style: {\n    base: {\n      iconColor: \"#000\",\n      color: \"#000\",\n      fontWeight: 500,\n      fontFamily: \"Roboto, Open Sans, Segoe UI, sans-serif\",\n      fontSize: \"16px\",\n      fontSmoothing: \"antialiased\",\n      \":-webkit-autofill\": { color: \"#fce883\" },\n      \"::placeholder\": { color: \"#808080\" },\n    },\n    invalid: {\n      iconColor: \"#ffc7ee\",\n      color: \"#ffc7ee\",\n    },\n  },\n};\n\nconst INIT_STATE = {\n  firstName: \"\",\n  lastName: \"\",\n  email: \"\",\n  address1: \"\",\n  address2: \"\",\n  city: \"\",\n  state: \"\",\n  zipcode: \"\",\n};\n\nconst CheckoutForm = ({ status, setStatus, total, clearCart }) => {\n  const stripe = useStripe();\n  const elements = useElements();\n  const [customerInfo, setCustomerInfo] = useState(INIT_STATE);\n  const history = useHistory();\n\n  const handleSubmit = async (event) => {\n    event.preventDefault();\n    setStatus(\"loading\");\n    const { error, paymentMethod } = await stripe.createPaymentMethod({\n      type: \"card\",\n      card: elements.getElement(CardElement),\n    });\n\n    if (!error) {\n      const { id } = paymentMethod;\n      try {\n        const { data } = await axios.post(\"http://localhost:5000/payments\", {\n          id,\n          amount: total,\n          customer: customerInfo,\n        });\n        setStatus(\"success\");\n        clearCart();\n        //clear cart and redirect to home page\n        // setTimeout(() => {\n        //   history.push(\"/\");\n        // }, 2000);\n      } catch (error) {\n        console.log(error);\n        setStatus(\"fail\");\n      }\n    } else {\n      console.log(error);\n      setStatus(\"fail\");\n    }\n  };\n\n  return (\n    <form onSubmit={handleSubmit} className=\"checkout-form\">\n      <CardElement options={CARD_OPTIONS} />\n      <div className=\"form-rows\">\n        <FormInput\n          name=\"email\"\n          type=\"email\"\n          value={customerInfo.email}\n          require={true}\n          label=\"email\"\n          handleChange={(e) =>\n            setCustomerInfo({ ...customerInfo, email: e.target.value })\n          }\n        />\n        <div className=\"name-row\">\n          <FormInput\n            name=\"firstName\"\n            type=\"text\"\n            value={customerInfo.firstName}\n            require={true}\n            label=\"John\"\n            handleChange={(e) =>\n              setCustomerInfo({ ...customerInfo, firstName: e.target.value })\n            }\n          />\n          <FormInput\n            name=\"lastName\"\n            type=\"text\"\n            value={customerInfo.lastName}\n            require={true}\n            label=\"Doe\"\n            handleChange={(e) =>\n              setCustomerInfo({ ...customerInfo, lastName: e.target.value })\n            }\n          />\n        </div>\n\n        <FormInput\n          name=\"address1\"\n          type=\"text\"\n          value={customerInfo.address1}\n          require={true}\n          label=\"Address 1\"\n          handleChange={(e) =>\n            setCustomerInfo({ ...customerInfo, address1: e.target.value })\n          }\n        />\n        <FormInput\n          name=\"address2\"\n          type=\"text\"\n          value={customerInfo.address2}\n          require={true}\n          label=\"Address 2 (optional)\"\n          handleChange={(e) =>\n            setCustomerInfo({ ...customerInfo, address2: e.target.value })\n          }\n        />\n        <div className=\"city-state-zip-row\">\n          <FormInput\n            name=\"city\"\n            type=\"text\"\n            value={customerInfo.city}\n            require={true}\n            label=\"city\"\n            handleChange={(e) =>\n              setCustomerInfo({ ...customerInfo, city: e.target.value })\n            }\n          />\n          <FormInput\n            name=\"state\"\n            type=\"text\"\n            value={customerInfo.state}\n            require={true}\n            label=\"state\"\n            handleChange={(e) =>\n              setCustomerInfo({ ...customerInfo, state: e.target.value })\n            }\n          />\n          <FormInput\n            name=\"zipcode\"\n            type=\"text\"\n            value={customerInfo.zipcode}\n            require={true}\n            label=\"Zipcode\"\n            handleChange={(e) =>\n              setCustomerInfo({ ...customerInfo, zipcode: e.target.value })\n            }\n          />\n        </div>\n      </div>\n      <div className=\"checkout-button\">\n        <Button\n          isLoading={status === \"loading\"}\n          className=\"submit\"\n          type=\"submit\"\n          disabled={!stripe}\n        >\n          Pay\n        </Button>\n      </div>\n    </form>\n  );\n};\n\nconst mapDispatchToProps = (dispatch) => ({\n  clearCart: () => dispatch(clearCart()),\n});\n\nexport default connect(null, mapDispatchToProps)(CheckoutForm);\n","/Users/seandan/ecommerce-app/src/components/loading-spinner/loading-spinner.styles.js",[],"/Users/seandan/ecommerce-app/src/components/loading-spinner/loading-spinner.component.js",[],"/Users/seandan/ecommerce-app/src/effects/use-fetch.effect.js",[],"/Users/seandan/ecommerce-app/src/utils/auth.js",[],"/Users/seandan/ecommerce-app/src/pages/profile/profile.component.js",[],"/Users/seandan/ecommerce-app/src/components/profile-main/profile-main.component.js",[],"/Users/seandan/ecommerce-app/src/components/collection-filter/collection-filter.component.js",[],"/Users/seandan/ecommerce-app/src/components/footer/footer.component.js",[],"/Users/seandan/ecommerce-app/src/components/scroll-to-top-button/scroll-to-top.component.js",[],"/Users/seandan/ecommerce-app/src/pages/product/product.component.js",[],"/Users/seandan/ecommerce-app/src/components/product-image/product-image.component.js",["249"],"import LoadingSpinner from \"../loading-spinner/loading-spinner.component\";\nimport \"./product-image.styles.scss\";\n\nconst ProductImage = ({ product }) => {\n  return (\n    <div className=\"product-image-container\">\n      <img src={product.imageUrl} alt=\"product image\"></img>\n    </div>\n  );\n};\nexport default LoadingSpinner(ProductImage);\n","/Users/seandan/ecommerce-app/src/components/product-info/product-info.component.js",[],"/Users/seandan/ecommerce-app/src/utils/collection.js",[],{"ruleId":"250","severity":1,"message":"251","line":3,"column":8,"nodeType":"252","messageId":"253","endLine":3,"endColumn":14},{"ruleId":"254","replacedBy":"255"},{"ruleId":"256","r